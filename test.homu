sub remove_arr_index {
    # index is idx_to_rem
    # array is arr_arg
    set i = 0;
    set out_arr = [];

    while i < arr_arg. {
        if i != idx_to_rem {
            out_arr += arr_arg|i|;
        }

        i += 1;
    }

    ret out_arr;
}

